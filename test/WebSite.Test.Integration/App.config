<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="meetup">
      <section name="signedEventUrls" type="System.Configuration.NameValueSectionHandler"/>
    </sectionGroup>
  </configSections>

  <meetup>
    <signedEventUrls>
      <add key="meetupgroupname1" value="http://api.meetup.com/2/events?meetupgroupname1"/>
      <add key="meetupgroupname2" value="http://api.meetup.com/2/events?meetupgroupname2"/>
    </signedEventUrls>
  </meetup>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
